{
    "name": "fantasy-academy/fantasy-academy",
    "type": "project",
    "description": "",
    "authors": [
        {
            "name": "Jan MikeÅ¡",
            "email": "j.mikes@me.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "runtime/frankenphp-symfony",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/php-runtime/frankenphp-symfony.git",
                    "type": "git",
                    "reference": "main"
                },
                "autoload": {
                    "psr-4": {
                        "Runtime\\FrankenPhpSymfony\\": "src/"
                    }
                },
                "require": {
                    "php": ">=8.1",
                    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0 || ^8.0",
                    "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0 || ^8.0",
                    "symfony/runtime": "^5.4 || ^6.0 || ^7.0 || ^8.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=8.5",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-simplexml": "*",
        "ext-uuid": "*",
        "api-platform/doctrine-orm": "^4.0",
        "api-platform/symfony": "^4.0",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "lexik/jwt-authentication-bundle": "^3.1",
        "monolog/monolog": "^3.0",
        "nelmio/cors-bundle": "^2.4",
        "nette/utils": "^4.0",
        "phpdocumentor/reflection-docblock": "^6.0",
        "phpoffice/phpspreadsheet": "^5.0",
        "phpstan/phpdoc-parser": "^2.2",
        "psr-discovery/cache-implementations": "^1.1",
        "psr-discovery/container-implementations": "^1.1",
        "psr-discovery/event-dispatcher-implementations": "^1.1",
        "psr-discovery/http-client-implementations": "^1.1",
        "runtime/frankenphp-symfony": "^1.0",
        "sentry/sentry-symfony": "^5.0",
        "stripe/stripe-php": "^19.1",
        "symfony/asset": "^8.0",
        "symfony/asset-mapper": "^8.0",
        "symfony/config": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/doctrine-messenger": "^8.0",
        "symfony/dom-crawler": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/expression-language": "^8.0",
        "symfony/flex": "^2.1",
        "symfony/form": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/http-client-contracts": "^3.4",
        "symfony/http-kernel": "^8.0",
        "symfony/intl": "^8.0",
        "symfony/mailer": "^8.0",
        "symfony/messenger": "^8.0",
        "symfony/mime": "^8.0",
        "symfony/monolog-bundle": "^4.0",
        "symfony/process": "^8.0",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/security-csrf": "^8.0",
        "symfony/serializer": "^8.0",
        "symfony/string": "^8.0",
        "symfony/translation": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/uid": "^8.0",
        "symfony/validator": "^8.0",
        "symfony/web-link": "^8.0",
        "symfony/yaml": "^8.0",
        "twig/cssinliner-extra": "^3.21",
        "twig/extra-bundle": "^3.21",
        "twig/inky-extra": "^3.21",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "justinrainbow/json-schema": "^6.6",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.2",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^8.0",
        "symfony/css-selector": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/maker-bundle": "^1.61"
    },
    "autoload": {
        "psr-4": {
            "FantasyAcademy\\API\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FantasyAcademy\\API\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan --memory-limit=-1 analyse",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "8.*",
            "docker": false
        }
    },
    "replace": {
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-uuid": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "endroid/installer": true,
            "php-http/discovery": false,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
